# ==============================================
# AiDareU - Production Environment Variables
# ==============================================
# Copy this file to .env and fill in your values

# Database (PostgreSQL)
POSTGRES_DB=aidareu_prod
POSTGRES_USER=aidareu_user
POSTGRES_PASSWORD=change-this-to-secure-password

# Laravel Backend
APP_KEY=base64:generate-this-with-php-artisan-key-generate

# Domains (Update with your actual domain)
DOMAIN=aidareu.com
API_DOMAIN=api.aidareu.com
